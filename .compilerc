{
  "env": {
    "development": {
      "text/typescript": {
        "removeComments": false,
        "preserveConstEnums": true,
        "declaration": true,
        "noImplicitAny": false,
        "noImplicitReturns": false,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": true,
        "noUnusedLocals": false,
        "noImplicitThis": true,
        "noUnusedParameters": false,
        "inlineSourceMap": true,
        "inlineSources": true,
        "importHelpers": true,
        "noEmitHelpers": true,
        "experimentalDecorators": true,
        "target": "es6",
        "module": "none",
        "jsx": "react"
      }
    },
    "production": {
      "text/typescript": {
        "removeComments": false,
        "preserveConstEnums": true,
        "declaration": true,
        "noImplicitAny": false,
        "noImplicitReturns": false,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": true,
        "noUnusedLocals": false,
        "noImplicitThis": true,
        "noUnusedParameters": false,
        "sourceMap": false,
        "importHelpers": true,
        "noEmitHelpers": true,
        "experimentalDecorators": true,
        "target": "es6",
        "jsx": "react"
      }
    }
  }
}